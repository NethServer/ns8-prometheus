{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "required": [
        "hosts"
    ],
    "additionalProperties": false,
    "properties": {
        "hosts": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string"
            }
        },
        "labels": {
            "type": "object",
            "minProperties": 1,
            "additionalProperties": false,
            "patternProperties": {
                "^.+$": {
                    "type": "string"
                }
            }
        }
    }
}
